- r = woodland.current_rank
%tr
  %td.col-xs-4
    - if can? :read, Bonum
      = link_to woodland.name, boy_path(woodland.slug)
    -else 
      = keep(woodland.name)

    = " (#{woodland.current_rank.grace})"
  %td.col-xs-1.right
    = r.service
    %small.text-muted hrs
  %td.col-xs-1.right= subdue_zero(r,'heritage')
  %td.col-xs-1.right= subdue_zero(r,'life_skills')
  %td.col-xs-1.right= subdue_zero(r,'sci_tech')
  %td.col-xs-1.right= subdue_zero(r,'hobbies')
  %td.col-xs-1.right= subdue_zero(r,'values')
  %td.col-xs-1.right= subdue_zero(r,'outdoor_activities')
  %td.col-xs-1.right= subdue_zero(r,'pioneer_skills')